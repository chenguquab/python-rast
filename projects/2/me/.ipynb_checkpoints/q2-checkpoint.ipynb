    "import pandas\n",
    "import folium\n",
    "\n",
    "df = pandas.read_csv(\"waterfalls.txt\",sep=\",\")\n",
    "ir = folium.Map(location=[32, 53], zoom_start=5.4)\n",
    "\n",
    "coordinates = df[[\"LATITUDE\",\"LONGITUDE\"]]\n",
    "for index,row in coordinates.iterrows():\n",
    "    folium.Marker(\n",
    "    [row[\"LATITUDE\"], row[\"LONGITUDE\"]]).add_to(ir)\n",
    "ir"
